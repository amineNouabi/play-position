{
  "expo": {
    "name": "Play Position",
    "slug": "play-position",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo1024x1024-white-bg.jpg",
    "scheme": "play-position",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo1024x1024-white-bg.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": ""
      },
      "bundleIdentifier": "com.aminenouabi.playposition"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aminenouabi.playposition",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo1024x1024.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Play Position to use your location.",
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ab3ae10d-b732-4e03-8223-25e1c7570bc4"
      }
    }
  }
}
