{
  "expo": {
    "name": "Play Position",
    "slug": "play-position",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo1024x1024-white-bg.jpg",
    "scheme": "play-position",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo1024x1024-white-bg.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1024x1024-white-bg.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aminenouabi.playposition"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo1024x1024.png"
    },
    "plugins": ["expo-router", "expo-secure-store", "expo-localization"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
